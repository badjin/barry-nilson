{
  "name": "bnlaw",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev.dev": "pm2 start ecosystem.config.js",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate-spa": "nuxt generate --spa ; cp robots.dev.txt dist/robots.txt",
    "generate-prod": "nuxt generate --changed",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "log.dev": "pm2 log nuxt2-craft3-starter",
    "prepare": "husky install",
    "test": "jest",
    "storybook": "npx nuxt storybook",
    "build-fragment": "NODE_TLS_REJECT_UNAUTHORIZED=0 API_ENDPOINT=https://cms.bnlaw.com.au node graph-ql/schemaQuery.js",
    "build-fragment-staging": "NODE_TLS_REJECT_UNAUTHORIZED=0 API_ENDPOINT=https://cms.bnlaw.com.au node graph-ql/schemaQuery.js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache",
    "*.{scss,sass,html,vue}": "stylelint",
    "*.{vue}": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@babel/runtime": "7.5.5",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/recaptcha": "^1.1.1",
    "@nuxtjs/svg": "^0.1.12",
    "@nuxtjs/toast": "^3.3.1",
    "@thirstcreative/components": "1.0.3",
    "@thirstcreative/nuxt-module": "^0.0.309",
    "algoliasearch": "^4.10.3",
    "browserify": "16.3.0",
    "cookie-universal-nuxt": "^2.2.2",
    "core-js": "^3.19.3",
    "form-data-json-convert": "^2.1.3",
    "fs": "^0.0.1-security",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
    "instantsearch.css": "^7.4.5",
    "instantsearch.js": "^4.27.2",
    "jimp": "^0.16.1",
    "jsonp": "^0.2.1",
    "lodash-es": "^4.17.21",
    "morphdom": "2.5.5",
    "mustache": "3.0.1",
    "nuxt": "^2.15.8",
    "parse-link-header": "^2.0.0",
    "png-to-ico": "^2.1.4",
    "postcss-nested": "^5.0.6",
    "pristinejs": "^0.1.9",
    "scss": "^0.2.4",
    "scss-loader": "^0.0.1",
    "shopify-buy": "2.12.0",
    "stylelint-config-tailwindcss": "^0.0.2",
    "uglify-js": "3.6.0",
    "v-click-outside": "^3.2.0",
    "vue": "^2.6.14",
    "vue-googlemaps": "^0.1.2",
    "vue-instantsearch": "^3.7.0",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vue2-google-maps": "^0.10.7",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@nuxt/image": "^0.7.1",
    "@nuxt/types": "~2.15.0",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/storybook": "^4.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^4.0.1",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "fast-xml-parser": "^4.0.1",
    "fibers": "^5.0.0",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.15",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.5.1",
    "sass": "^1.43.4",
    "sass-loader": "10",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "vue-jest": "^3.0.4"
  }
}
